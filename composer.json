{
    "name": "clivern/monkey",
    "type": "library",
    "description": "Apache CloudStack SDK in PHP.",
    "keywords": ["clivern", "monkey", "cloudstack", "apache-cloudstack", "cloudstack-api", "cloud-computing"],
    "license": "MIT",
    "homepage": "https://github.com/clivern/monkey",
    "authors": [
        {
            "name": "Clivern",
            "email": "hello@clivern.com",
            "homepage": "http://clivern.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "friendsofphp/php-cs-fixer": "^2.14",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Clivern\\Monkey\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Clivern\\Monkey\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
